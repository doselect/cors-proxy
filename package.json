  "author": "DoSelect",
  "repository": {
    "type": "git",
    "url": "https://github.com/doselect/cors-proxy.git"
  },
  "bugs": {
    "url": "https://github.com/doselect/cors-proxy/issues/",
    "email": "kokil@doselect.com"
  },
  "keywords": [
    "cors",
    "cross-domain",
    "http-proxy",
    "proxy",
    "heroku"
  ],
