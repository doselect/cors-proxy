    "test/",
    "Procfile",
    "demo.html",
    "server.js"
  ],
  "dependencies": {
    "cheerio": "^0.22.0",
    "express": "^4.17.1",
    "harmon": "^1.4.3",
    "http-proxy": "1.11.1",
    "netlify-lambda": "^1.5.0",
    "parse5": "^5.1.0",
    "parse5-html-rewriting-stream": "^5.1.0",
    "proxy-from-env": "0.0.1",
    "serverless-http": "^2.0.2"
  },
  "devDependencies": {
    "eslint": "^6.0.1",
    "istanbul": "^0.4.5",
    "lolex": "^1.5.0",
    "mocha": "^6.1.4",
    "nock": "^8.2.1",
    "supertest": "^4.0.2"
  },
  "scripts": {
